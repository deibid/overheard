    <%= form_for @podcast, :html => { :multipart => true } do |f|  %>
    <%= f.label :title %> <br> 
    <%= f.text_field :title %> <br>
    <%= f.label :genre %> <br>
    <%= f.select :genre, Podcast::GENRES %> <br>
    <%= f.label :author %> <br>
    <%= f.text_field :author %> <br>
    <%= f.label :mood %> <br>
    <%= f.select :mood, Podcast::MOODS %> <br>
    <%= f.label :length %> <br>
    <%= f.text_field :length %> <br>
    <%= f.label :cover_photo %> <br>
    <%= f.file_field :cover %> <br>
    <%= f.label :audio %>
    <%= f.file_field :audio %>
    <%= f.label :tags!, "Tags (separated by commas dudes!" %> <br> 
    <%=  f.text_field :tag_list %> <br>   
    <%= f.submit "Upload that shit" %>
  <% end %>
